{
  "name": "hamburger",
  "custom": {
    "cors": {
      "allowOrigin": "*",
      "allowHeaders": ["Content-Type", "X-Amz-Date", "Authorization", "X-Api-Key"]
    },
    "optimize": {
      "transforms": [{
        "name": "babelify",
        "opts": {
          "presets": [
            "es2015"
          ]
        }
      }]
    }
  },
  "plugins": [
    "serverless-cors-plugin",
    "serverless-optimizer-plugin",
    "serverless-mocha-plugin"
  ]
}